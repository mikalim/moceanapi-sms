<?xml version="1.0"?>
<extension key="org.civicrm.sms.moceanapi" type="module">
  <file>moceanapi_sms</file>
  <name>MoceanAPI SMS Provider</name>
  <description>MoceanAPI extension provides SMS service for users deliver individual, bulk and scheduler SMS messages in every country around the world. </description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>MoceanAPI</author>
    <email>plugin@moceanapi.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/moceanapi/org.civicrm.sms.moceanapi</url>
    <url desc="Documentation">https://github.com/moceanapi/org.civicrm.sms.moceanapi</url>
    <url desc="Support">https://moceanapi.com/support</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-04-14</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>For support, please contact project team on https://moceanapi.com/support</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/MoceanapiSms</namespace>
  </civix>
</extension>
